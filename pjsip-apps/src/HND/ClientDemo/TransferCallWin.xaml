<Window x:Class="ClientDemo.TransferCallWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="转接" Height="186" Width="312" ShowInTaskbar="False" ResizeMode="NoResize" WindowStyle="ToolWindow">
    <Grid>
        <Label x:Name="labSys" Content="系统ID：" HorizontalAlignment="Left" Margin="47,26,0,0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Right" />
        <TextBox x:Name="txt_SystemID" Height="23" Width="132" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="107,29,0,0"/>
        <Label x:Name="labNum" Content="号码：" HorizontalAlignment="Left" Margin="52,57,0,0" VerticalAlignment="Top" Width="55" HorizontalContentAlignment="Right" />
        <TextBox x:Name="txtCallNumber" HorizontalAlignment="Left" Height="23" Margin="107,57,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="132"/>
        <Button x:Name="btnCall" Content="呼 叫" HorizontalAlignment="Left" Margin="63,104,0,0" VerticalAlignment="Top" Width="75" Click="btnCall_Click"/>
        <Button x:Name="btnTransfer" Content="完成转接" HorizontalAlignment="Left" Margin="164,104,0,0" VerticalAlignment="Top" Width="75" Click="btnTransfer_Click"/>

    </Grid>
</Window>
